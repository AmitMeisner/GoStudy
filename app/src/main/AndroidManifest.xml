<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.goStudy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.goStudy.Practice2"></activity>
        <activity android:name="com.example.goStudy.TipsActivity" />
        <activity android:name="com.example.goStudy.StatActivity" />
        <activity android:name="com.example.goStudy.ProgressActivity" />
        <activity android:name="com.example.goStudy.DailyActivity" />
        <activity android:name="com.example.goStudy.HomeActicity" />
        <activity android:name="com.example.goStudy.Practice" />
        <activity android:name="com.example.goStudy.WelcomeActivity" android:noHistory="true"/>
        <activity
            android:name="com.example.goStudy.MainNavActivity"
            android:label="@string/title_activity_main_nav"
            android:theme="@style/AppTheme.NoActionBar"
            />
        <activity
            android:name="com.example.goStudy.MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>